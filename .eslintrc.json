{
  "env": {
    "browser": true
  },
  "extends": [
    "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions":  {
    "parser": "@typescript-eslint/parser",
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "semi": "error",
    "@typescript-eslint/indent": "off",
    "@typescript-eslint/no-unused-vars": "error"
  }
}
